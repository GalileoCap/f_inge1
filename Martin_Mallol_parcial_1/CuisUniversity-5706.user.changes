	vecesQueGolpeoCapaActual := vecesQueGolpeoCapaActual + 1.
	
	^self capaDeSueloInmediata perforadoraDaGolpeDeTaladro: self .
	
	"self sobreSueloArenoso ifTrue: [
		mechaPuesta = #Soft ifTrue: [ ^self pasarAProximaCapa ].
