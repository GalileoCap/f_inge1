!Perforadora methodsFor: 'scan' stamp: 'mm 5/8/2023 20:09:34' prior: 50608730!
calcularProfundidadDelSueloHasta: tipoDeCapa
	
	| valorDeCapa acc ix |
	
	valorDeCapa := tipoDeCapa scan.
	
	acc := 0.
	ix := 1.
	
	[ix <= capasDeSueloDebajo size] whileTrue: [ | capa altura |
		capa := capasDeSueloDebajo at: ix.
		
		valorDeCapa = capa scan ifTrue: [ ^ acc ].
		
		altura := capa altura.
		altura = nil ifTrue: [ altura := 0 ].
		acc := acc + altura.
		ix := ix + 1.		
	].

	^ acc.
	
	 ! !

